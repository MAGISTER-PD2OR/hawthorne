.paginationContent
  .innerMain
    h1 Players
    .CBox.nomargin
      .timeTableGo.fRight(onclick="window.ajax('admins[servers][admins]', '.paginationContent table tbody', window.pagination.current + 1, true)")
        i.icon.ion-ios-arrow-forward
      .timeTableGo.fLeft(onclick="window.ajax('admins[servers][admins]', '.paginationContent table tbody', window.pagination.current - 1, true)")
        i.icon.ion-ios-arrow-back
      h3.center
        span Page
        span.current 1
        span /{{pages}}
      table.timeTable
        thead
          tr
            th
            th Username
            th Connection Time
            th Punishments
            th Messages
        tbody
  .innerSide
    h1 Options
    .CBox.nomargin
      h3
        i.icon.ion-ios-funnel(style='margin-right:10px')
        |  Search Filters
        button(style="margin-top: -13px;float: right;width: auto;margin-right: -13px;") Edit
      p Displaying active search filters:
      ul.searchTags
        li
          b Name
          |  – Bob
          i.icon.ion-ios-close
        li
          b Country
          |  – Germany
          i.icon.ion-ios-close
        li
          b Connection Time
          |  – more than 24 hours
          i.icon.ion-ios-close
        li
          b Punishments
          |  – more 12
          i.icon.ion-ios-close
        li
          b Messages
          |  – more 24
          i.icon.ion-ios-close

  script.execute.
    window.ajax("players[overview]", ".paginationContent table tbody", {{current}}, true)
    window.pagination = {
      'current': {{current}},
      'limitation': {{pages}}
    }
