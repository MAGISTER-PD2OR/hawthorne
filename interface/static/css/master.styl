.navActive
  opacity 1 !important
  background $color-21 !important
  color $color-11 !important
  border-color transparent !important
  /* border-left: 3px solid #E9D460 !important; */

  .icon
    /* color: #E9D460; */

  a
    color $color-3 !important

.wrap
  position relative
  float left
  top 50%
  left 50%
  -webkit-transform translate(-50%, -50%)
  transform translate(-50%, -50%)

  h1
    position relative
    z-index 2
    color $color-3
    text-transform uppercase

.center
  text-align center

.pre
  height 800px
  width 1200px
  margin -100px

.tooltip
  position absolute
  z-index 99
  padding 2px 10px
  background $color-22
  font-family inherit
  font-weight 600
  color $color-3
  text-align center
  border-radius 4px

  &:after
    content ''
    position absolute
    bottom -5px
    left 50%
    height 0
    width 0
    margin-left -5px
    border-top 5px solid $color-22
    border-right 5px solid transparent
    border-left 5px solid transparent

  &.top
    &:after
      top -10px
      bottom auto
      border-top-color black
      border-bottom 5px solid $color-22

  &.left
    &:after
      left 10px
      margin 0

  &.right
    &:after
      right 10px
      left auto
      margin 0

.main
  float right
  width 85%
  padding 40px
  color white

  table
    border-collapse collapse
    border-style hidden
    border-radius 4px

    tr
      &:last-child
        /* border-radius:30px; */

  th
    padding 15px
    color $color-29


  thead
    tr
      background $color-12
      color $color-32
      font-weight 300
      /* border-bottom: 1px solid rgba(0, 169, 255, 0.5); */

  tbody
    tr
      width 100%
      margin 20px 0
      padding 15px
      border-bottom none
      transition .3s all ease-in-out

    td
      padding 12px
      font-weight 300


    tbody
      tr
        &:hover
          /* background: rgba(0, 0, 0, 0.15); */

h1
  margin 20px 0
  padding 0
  font-size 1.3em

  &:first-child
    margin-top 0 !important

  button
    float right
    font-weight 300

h2
  margin 0 0 20px 0
  padding 0
  font-weight 300

h3
  margin 0 0 20px 0
  padding 0
