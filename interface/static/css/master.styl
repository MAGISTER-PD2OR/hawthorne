.slider
  position absolute
  top 0
  right 0
  bottom 0
  left 0
  cursor pointer
  background-color $color-12
  transition .4s

  &:before
    content ""
    position absolute
    bottom 4px
    left 4px
    height 21px
    width 21px
    background-color white
    transition .4s

  &.round
    border-radius 34px

    &:before
      border-radius 50%

input
  &:checked
    +.slider
      background $color-58

      &:before
        -webkit-transform translateX(26px)
        transform translateX(26px)

  &:focus
    +.slider
      box-shadow 0 0 1px $color-52

