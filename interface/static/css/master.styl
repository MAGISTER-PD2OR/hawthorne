.overview
  float left
  width 100%
  margin 30px 0 40px 0
  text-align center

  .icon
    display list-item
    float left
    margin 7px 20px -7px 0
    font-size 48px
    color $color-23
    list-style-type none

  .title
    display block
    margin 30px 0 0 0
    font-size 1.3em
    font-weight bold

  .count
    display inline
    margin-right 5px
    font-size 20px
    font-weight 400
    font-weight bold
    color $color-23

  .info
    display block
    margin 0
    margin-top 10px
