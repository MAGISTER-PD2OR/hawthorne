.col
  display block
  float left
  margin 0 0 1.6% 1.6%

  &:first-child
    margin-left 0

  &.avatar
    overflow hidden
    width calc(50% - 20px)
    /*border-radius: calc(50%);*/
    margin 0 20px 0 0 !important
    border-radius 12px
    border-radius 3px

    img
      width 100% !important
      margin 0 !important

.group
  zoom 1

  &:before
    content ""
    display table

  &:after
    content ""
    display table
    clear both

.span_4_of_4
  width 100%

.span_3_of_4
  width 74.6%

.span_2_of_4
  width 49.2%

.span_1_of_4
