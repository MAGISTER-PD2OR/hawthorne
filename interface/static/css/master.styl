    font-family inherit
    font-size inherit
    text-align center
    font-weight bold
    color $color-9
    border none
    box-shadow 0 5px 10px -5px $color-12
    outline none
    transition .3s all ease-in-out
    -moz-box-shadow 0 5px 10px -5px rgba(0, 0, 0, .1)

    &.first
      border-bottom-left-radius 2px
      border-top-left-radius 2px
      border-right 1px solid $color-48

    &.last
      border-top-right-radius 2px
      border-bottom-right-radius 2px
      border-left 1px solid $color-48

  &:checked + label
    background $color-25 !important

textarea
