html
  height 100%
  background-color $color-2

body
  min-height 100%

  display block
  display flow-root

  margin 0
  background $color-1
  background linear-gradient(to bottom left, $color-2, $color-1)
  background-attachment fixed
  background-repeat no-repeat
  font-family 'Rubik', sans-serif
  font-size 13px
  font-weight 300
  color $color-3
  -webkit-font-smoothing antialiased

*
  box-sizing border-box
  outline none

/**:first-letter {
  text-transform: uppercase;
}*/

a
  color inherit
  text-decoration inherit

code
  padding 2px 5px
  background $color-4
  color white
  line-height 2
  border-radius 4px

.loadingOverlay
  position fixed
  top 0
  right 0
  bottom 0
  left 0
  z-index 95
  height 100%
  width 100%
  background $color-5

  >.loadingContainer
    position absolute
    top 50%
    left 50%
    -webkit-transform translate(-50%, -50%)
    transform translate(-50%, -50%)

    >.hawthorne
      margin 1px 0 0 10px
      font-size 60px

.menu
  position relative
  /* border-right: 2px solid rgb(20, 35, 78); */
  position fixed
  float left
  z-index 1
  overflow-y auto
  height 100%
  width 15%
  background $color-6
  background linear-gradient(to bottom, $color-2, $color-1)
  background -moz-linear-gradient(to bottom, $color-6, $color-7)
  box-shadow -5px 0 50px -5px $color-8
  -moz-box-shadow -5px 0 50px -5px rgba(0, 0, 0, .3)

  .icon
    width 40px
    float left
    /* border-right: 1px solid rgba(255, 255, 255, 0.03); */
    margin -10px 10px -10px 0
    padding 10px 20px 7px 0
    font-size 21px
    text-align center

  >ul
    margin 0
    padding 0
    list-style-type none

    >li
      >a
        display inline-block
        width 100%
        padding 20px
        font-weight bold
        color $color-9
        line-height 1.4
        /* border-left: 3px solid transparent; */
        border-bottom none
        border-bottom 1px solid $color-10
        outline none
        transition .3s
        transition all .3s ease-in-out

        >.feather
          font-size 25px

        &:hover
          opacity 1 !important
          color $color-11 !important
          background $color-12
          /* border-color:#E9D460; */

          .icon
            /* color: #E9D460; */

      >.expand
        float left
        width 100%
        background $color-13
        display none
        /* border-left: 3px solid #E9D460; */
        box-shadow inset 0 7px 20px -7px $color-14
        -moz-box-shadow inset 0 7px 20px -7px rgb(9, 35, 66)

        >ul
          display flow-root
          margin 0
          padding 10px
          color $color-15
          list-style-type none

          >li
            >a
              display block
              margin 0 -10px
              padding 10px 50px
              font-weight 300
              transition .3s all ease-in-out

              &:hover
                background $color-10 !important
                color $color-16


    >.title
      width 100%

      .feather
        float right
        height 17px
        margin 1px 0

  .more
    float right
    width auto
    width 40px
    margin -8px -7px
    padding 10px
    background $color-12
    font-size 17px
    color $color-18
    text-align center
    border none
    border-radius 2px

  .user
    position relative
    float left
    width 100%
    margin 0
    padding 20px
    background $color-10
    color $color-18
    box-shadow inset 0 -10px 10px -10px $color-12
    transition .3s all ease-in-out
    -moz-box-shadow inset 0 -10px 10px -10px rgba(0, 0, 0, .1)
    -webkit-box-shadow inset 0 -10px 10px -10px rgba(0, 0, 0, .1)

    .dropdown
      position absolute
      display none
      top 57px
      right 0
      width 250px
      background $color-4
      border-radius 0 0 2px 2px

      >ul
        margin 0
        padding 10px
        list-style-type none

        >li
          >a
            display block
            width 100%
            padding 10px 15px
            font-weight 300
            color $color-15
            border-radius 2px
            transition .3s all ease-in-out

            &:hover
              background $color-19
              color white

    .username
      font-size 15px
      font-weight bold
      color $color-9
      line-height 1.5

    img
      float left
      height 42px
      margin-right 15px
      border-radius 50%

    >.icon
      margin-left 10px

  .links
    display block
    clear both
    width 100%
    margin-bottom 10px
    padding 0 0 20px 0
    border-bottom 1px solid $color-20

    .icon
      margin 0
      font-size 25px

    >ul
      margin 0
      padding 0
      text-align center
      list-style-type none

      >li
        display inline-block
        padding 0 20px

        a
          transition .05s all ease-in-out

          &:hover
            color $color-3

